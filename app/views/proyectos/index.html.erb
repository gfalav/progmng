<h1>Listing proyectos</h1>

<table>
  <thead>
    <tr>
      <th>Empresa</th>
      <th>Nombre</th>
      <th>Description</th>
      <th>Comitente</th>
      <th>Lider</th>
      <th>Falta</th>
      <th>Fuce</th>
      <th>Fcierre</th>
      <th>Logofile</th>
      <th>Eproyecto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proyectos.each do |proyecto| %>
      <tr>
        <td><%= proyecto.empresa_id %></td>
        <td><%= proyecto.nombre %></td>
        <td><%= proyecto.description %></td>
        <td><%= proyecto.comitente %></td>
        <td><%= proyecto.lider %></td>
        <td><%= proyecto.falta %></td>
        <td><%= proyecto.fuce %></td>
        <td><%= proyecto.fcierre %></td>
        <td><%= proyecto.logofile %></td>
        <td><%= proyecto.eproyecto %></td>
        <td><%= link_to 'Show', proyecto %></td>
        <td><%= link_to 'Edit', edit_proyecto_path(proyecto) %></td>
        <td><%= link_to 'Destroy', proyecto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proyecto', new_proyecto_path %>
